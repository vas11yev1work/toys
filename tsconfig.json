{
  "compilerOptions": {
    "sourceMap": true,
    "watch": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@pinia/nuxt"
    ]
  },
  "extends": "./.nuxt/tsconfig.json",
  "exclude": [
    "node_modules"
  ]
}
